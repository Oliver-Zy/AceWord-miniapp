.tab-bar {
  width: 100%;
  height: 48px;
  display: flex;
  position: fixed;
  bottom: 0;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.tab-bar-item cover-image {
  width: 24px;
  height: 24px;
  padding-bottom: 2px;
}

.tab-bar-item cover-view {
  font-family: var(--simhei);
  font-weight: 400;
  font-size: 10px;
}