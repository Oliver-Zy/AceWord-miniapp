@import '/templates/diccard-swiper.wxss';

.container {
  width: 100%;
  height: 100%;
  background-image: linear-gradient(180deg, #C5D6FA -10%, #F3F4F7 30%)
} 
.empty{
  text-align: center;
  padding-top: 10vh;
}
scroll-view {
  width: 100%;
  height: 100%;
}

@media (prefers-color-scheme: dark) {

  /* DarkMode 下的样式 start */
  .container {
    width: 100%;
    height: 100%;
    background: #1D1E30;
  }
  /* DarkMode 下的样式 end */
  
}