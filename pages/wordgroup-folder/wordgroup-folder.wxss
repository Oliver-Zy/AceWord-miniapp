/* pages/wordBookFolder/wordBookFolder.wxss */
.container {
  width: 100%;
  height: 100%;
  background:#FDFDFD;
}

scroll-view {
  width: 100%;
  height: 100%;
  background:#FDFDFD;
}

.field-container {
  box-sizing: border-box;
  width: 100%;
  margin: 28px 0 40px 0;
  padding: 0 24px;
}

@media (prefers-color-scheme: dark) {
  /* DarkMode 下的样式 start */
  .container {
    width: 100%;
    height: 100%;
    background:#1D1E30;
  }

  scroll-view {
    width: 100%;
    height: 100%;
    background:#1D1E30;
  }  
  /* DarkMode 下的样式 end */
}